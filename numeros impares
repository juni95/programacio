{

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main ()
{
   int a,b,c,prod,sum;
   cin>>a;
   cin>>b;
   if (a%2!=0&& b%2!=0)
   {
     sum=a+b;
     cout<<sum;
   }
   else
   {
     prod=a*b;
   }
   getch();

}
