 #include "stdafx.h"
    #include <iostream>
    #include "conio.h"
    using namespace std;  
	void main()
	{  
        int a,b,c;  
        cout<<("Ingresa a ");  
        cin>>a;  
        cout<<("Ingresa b ");  
        cin>>b;  
        cout<<("Ingresa c ");  
        cin>>c;
        if(a>b)
		  if(a>c)
		     cout<<"El mayor es:"<<a;
		  else 
		     cout<<c;
		else
		   if(b>c) 
			 cout<<b;
		   else 
			 cout<<c;
		
	         cout<<"El mayor es:"<<a;
        
		getch(); 
	}  
       
